{
  "scripts": {
    "start": "concurrently \"npm run dev:auth\" \"npm run dev:property\"",
    "dev:auth": "npm --prefix services/auth-service run dev",
    "dev:property": "npm --prefix services/property-service run dev"
  },
  "private": true,
  "workspaces": [
    "services/auth-service",
    "services/property-service",
    "shared"
  ],
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  }
}
